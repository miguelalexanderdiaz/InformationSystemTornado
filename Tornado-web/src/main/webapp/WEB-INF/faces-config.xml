<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/inventario/tabla_inventario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
        <navigation-rule>
        <from-view-id>/login/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/login/admin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>disenador</from-outcome>
            <to-view-id>/login/disenador.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>